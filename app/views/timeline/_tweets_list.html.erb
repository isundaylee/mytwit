<% if tweets.any? %>

<% tweets.each do |tweet| %>

<hr />
<div class="row">
	<div class="span1">
		<img src="data:image/jpeg;base64," />
	</div>
	<div class="span11">
		<p class="tweet-author"><%= tweet.user.name %><small class="tweet-timecode pull-right"><%= tweet.created_at %></small></p>
		<p class="tweet-text"><%= html_codify tweet.content %></p>
	</div>
	
	<div class="row">
		<div class="span12">
			<a class="btn btn-mini pull-right tweet-button">Retweet</a>
			<a class="btn btn-mini pull-right tweet-button">Comment</a>
		</div>
	</div>
</div>

<% end %>

<hr />

<%= will_paginate tweets %>

<% else %>

<hr />
<h3 class="center">There are no tweets yet. </h3>
<hr />

<% end %>